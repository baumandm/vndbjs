{
  "name": "vndbjs",
  "version": "0.3.3",
  "description": "A Node.js library for accessing the VNDB.org database.",
  "main": "vndb.js",
  "scripts": {
    "start": "node src/vndb.js",
    "docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
