  "bugs": {
    "url": "https://github.com/arbauman/vndbjs/issues"
  },
  "homepage": "https://github.com/arbauman/vndbjs#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "docdash": "^0.4.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.3",
    "mocha": "^3.1.2"
  },
  "dependencies": {
    "defaults-deep": "^0.2.3",
    "defaults-shallow": "^0.1.0",
