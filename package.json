{
  "name": "vndbjs",
  "version": "1.0.1",
  "description": "A node.js library for the Visual Novel Database.",
  "main": "/src/Vndb.js",
  "scripts": {
    "start": "node src/vndb.js",
    "test": "npm run lint && npm run cover && npm run docs",
    "docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "lint": "./node_modules/.bin/eslint src/",
    "cover": "node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- -R spec"
  },
  "files": [
    "package.json",
    "readme.md",
    "license",
    "contributing.md",
    "/src"
  ],
  "repository": {
    "type": "git",
    "url": "https://arbauman@gitlab.com/arbauman/vndbjs.git"
  },
  "keywords": [
    "vndb",
    "visual",
    "novel",
    "database",
    "api"
  ],
  "author": "Arbauman <arbauman207@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/arbauman/vndbjs/issues"
  },
  "homepage": "https://gitlab.com/arbauman/vndbjs/blob/master/readme.md",
  "dependencies": {
    "defaults-shallow": "^0.1.0",
    "generic-pool": "^3.1.7",
    "limiter": "^1.1.0",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "docdash": "^0.4.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.3",
    "mocha": "^4.0.1"
  }
}
